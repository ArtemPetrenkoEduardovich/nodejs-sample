  "main": "index.js",
  "scripts": {
    "start": "tsc && ts-node -r tsconfig-paths/register dist/index.js",
    "devel": "npx nodemon -r tsconfig-paths/register --exec ts-node ./src/index.ts",
    "build": "tsc --project tsconfig.build.json",
    "test": "tsc && mocha --timeout 30000 --exit src/test/**/*.test.ts --require ts-node/register -r tsconfig-paths/register",
    "coverage": "nyc mocha --timeout 30000 --exit src/test/**/*.test.ts --require ts-node/register -r tsconfig-paths/register",
    "lint": "eslint . --ext .ts"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 82
  },
  "dependencies": {
    "chai-http": "4.3.0",
    "consul": "1.2.0",
    "express": "4.18.2",
    "log4js": "6.9.1",
    "mongoose": "7.0.4"
  },
  "devDependencies": {
    "@types/chai": "4.3.5",
    "@types/consul": "0.40.0",
    "@types/express": "4.17.17",
    "@types/mocha": "10.0.1",
    "@types/node": "16.11.6",
    "@types/sinon": "10.0.14",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "chai": "4.3.7",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "http-status": "1.6.2",
    "mocha": "10.2.0",
    "mongodb-memory-server": "8.12.2",
    "nodemon": "2.0.22",
    "nyc": "15.1.0",
    "sinon": "15.0.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.4.4"
  }
}
